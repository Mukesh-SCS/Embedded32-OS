{
  "name": "@embedded32/sdk-js",
  "version": "1.0.0",
  "description": "J1939 client library for interacting with Embedded32 platform",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
