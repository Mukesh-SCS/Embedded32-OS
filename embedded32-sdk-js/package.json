    "url": "https://github.com/Mukesh-SCS/Embedded32.git",
    "directory": "embedded32-sdk-js"
  },
  "dependencies": {
    "@embedded32/j1939": "*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-node": "^10.9.0"
  }
